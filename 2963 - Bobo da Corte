#include <stdio.h>

 int main() {

int n, i, votos, bobo, m;

   m = 0;

   scanf("%d", &n);

   for (i = 1; i <= n ; i++){
     scanf("%d", &votos);
     if (i == 1){
       bobo = votos;
     }else{
       if (votos > bobo){
         m++;
       }else{
         
       }
     }
   }

   if (m == 0){
     printf("S\n");
   }else{
     printf("N\n");
   }
   
   
return 0;

}
