#include <stdio.h>

int main() {

int n;

  scanf ("%d", &n);

  if (n == 0){
    printf ("E\n");
  }else{
    if (0 < n && n <= 35){
      printf ("D\n");
    }else{
      if (35 < n && n <= 60){
        printf ("C\n");
      }else{
        if (60 < n && n <= 85){
          printf ("B\n");
        }else{
          printf ("A\n");
        }
      }
    }
  }

return 0;
}
