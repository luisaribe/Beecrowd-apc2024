#include <stdio.h>
#define MAX_N 1000

 int main() {

int n, i, m;
   int arr[MAX_N];

   scanf("%d", &n);

   for (i = 0; i < 1000; i++){

     if (i%n == 0|| i == 0){
       arr[i] = 0;
       m = arr[i];
     }else{
       arr[i] = ++m;
       m = arr[i];
     }
     printf("N[%d] = %d\n", i, arr[i]);
   }
   
return 0;

}
