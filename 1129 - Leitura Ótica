#include<stdio.h>

int main(){

int i, arr[6], n;

scanf("%d", &n);

  while(n != 0){ 

for(i=1; i<=n; i++){

  scanf("%d %d %d %d %d", &arr[1], &arr[2], &arr[3], &arr[4], &arr[5]);

  if(arr[1] <= 127 && arr[2]> 127 && arr[3]> 127 && arr[4]> 127 && arr[5]> 127){
    printf("A\n");
  }else{
    if(arr[2] <= 127 && arr[1]> 127 && arr[3]> 127 && arr[4]> 127 && arr[5]> 127){
      printf("B\n");
    }else{
      if(arr[3] <= 127 && arr[1]> 127 && arr[2]> 127 && arr[4]> 127 && arr[5]> 127){
        printf("C\n");
    }else{
        if(arr[4] <= 127 && arr[1]> 127 && arr[3]> 127 && arr[2]> 127 && arr[5]> 127){
          printf("D\n");
    }else{
          if(arr[5] <= 127 && arr[1]> 127 && arr[3]> 127 && arr[4]> 127 && arr[2]> 127){
            printf("E\n");
    }else{
            printf("*\n");
    }
  }
      }}}}

      scanf("%d", &n);
      
    }

return 0;

}
