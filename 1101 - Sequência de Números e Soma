#include <stdio.h>

 int main() {

int n, i, j, m, s, p, b;

   s = 0;
   p = 0;

   for (i = 1; ; i++){
     scanf("%d %d", &m, &n);
     if (m <= 0 || n <= 0){
       break;
     }
     if (m > n){
       for(j=n; j<=m; j++){
         printf("%d ", j);
         s = s + j;
       }
       printf("Sum=%d\n", s);
       s = 0;
     }else{
       if (m < n){
          for(b=m; b<=n; b++){
            printf("%d ", b);
            p = p + b;
            
          }
         printf("Sum=%d\n", p);
         p = 0;
     }  }
   }
   
   
return 0;

}
